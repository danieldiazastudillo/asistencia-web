{
  "name": "asistencia-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-default": "npm run build:prerender",
    "lint": "ng lint asistencia-web",
    "build:client-and-server-bundles": "ng build --prod && ng run asistencia-web:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "6.1.10",
    "@angular/router": "^6.1.10",
    "@hackages/ngxerrors": "6.0.3",
    "@ng-bootstrap/ng-bootstrap": "3.2.0",
    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "@nicky-lenaers/ngx-scroll-to": "1.0.0",
    "bootstrap": "4.1.3",
    "bourbon": "5.1.0",
    "core-js": "^2.5.7",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "ngx-bootstrap": "3.0.1",
    "popper.js": "1.14.4",
    "rxjs": "^6.3.3",
    "rxjs-compat": "6.3.3",
    "ts-loader": "4.5.0",
    "vegas": "2.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.0-beta.2",
    "@angular-devkit/schematics": "0.7.3",
    "@angular/cli": "^6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.5",
    "check-dependencies": "1.1.0",
    "codelyzer": "~4.3.0",
    "express": "4.16.3",
    "http-server": "0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-cli": "3.1.0"
  }
}
